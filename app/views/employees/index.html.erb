<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Birthdate</th>
    <th>SSN</th>
  </tr>
  <% @employees.each do |employee| %>
    <tr>
      <td><%= employee.first_name %></td>
      <td><%= employee.last_name %></td>
      <td><%= employee.birthdate %></td>
      <td><%= employee.ssn %></td>
    </tr>
  <% end %>
</table>